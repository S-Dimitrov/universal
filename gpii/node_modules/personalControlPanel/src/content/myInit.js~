(function ($, fluid) {
 
    //initialize the UI Enhancer
    var initUIEnhancer = function () {
 
        var enhancerOpts = {
            defaultSiteSettings: {
                textFont: "10",
    									textSize: "10"
            },
            tableOfContents: {
                options: {
                    templateUrl: "infusion-1.4/components/tableOfContents/html/TableOfContents.html"
                }
 
            }
        };
 
        return fluid.uiEnhancer(document, enhancerOpts);
    };
	    $(document).ready( function () {
        initUIEnhancer();
    });
})(jQuery, fluid);